<Window x:Class="KikeletPanzio_PocsaiG.VisszaTeroWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KikeletPanzio_PocsaiG"
        mc:Ignorable="d"
        Title="VisszaTeroWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Label Content="Email cím:" Margin="20,10,0,0"/>
            <TextBox Name="txtBoxVisszateroEmail" Width="200" Height="25" Margin="0,5,0,0"/>
            <Button Name="btnVisszateroBejelentkezes" Click="btnVisszateroBejelentkezes_Click" Height="30" Width="270" Margin="0,20,0,0" Content="Felhasználó bejelentkeztetése" FontSize="19"/>
        </StackPanel>
    </Grid>
</Window>
